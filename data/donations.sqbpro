	, r.name as register
FROM register_work as rw
LEFT JOIN register as r ON r.id = rw.register_id
LEFT JOIN work as w ON w.id = rw.work_id
WHERE w.name = 'Hauptwerk'</sql><sql name="master-table">SELECT
	o.id,
	r.name AS Register,
	t.name AS Typ,
	s.name AS Ton,
	sp.name AS Patenschaft,
	o.price AS Preis
	FROM organpipes AS o
	LEFT JOIN register AS r ON r.id = o.register_id
	LEFT JOIN type AS t ON t.id = o.type_id
	LEFT JOIN register_sound AS rs ON rs.id = o.id
	LEFT JOIN sound AS s ON s.id = rs.sound_id
	LEFT JOIN sponsorship AS sp ON sp.id = o.sponsorship_id
	-- WHERE Patenschaft = 'vergeben' AND Register = &quot;Oktave 4'&quot;</sql><sql name="get-user">SELECT id, 
